            <Label fx:id="lbl_host" layoutX="29.0" layoutY="174.0" text="Host:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lbl_port" layoutX="30.0" layoutY="202.0" text="Port:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lbl_username" layoutX="30.0" layoutY="235.0" text="Username:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lbl_password" layoutX="30.0" layoutY="269.0" text="Password:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lbl_database" layoutX="29.0" layoutY="301.0" text="Database:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <!--values-->
            <Label fx:id="txt_host" layoutX="125.0" layoutY="174.0" text=""/>
            <Label fx:id="txt_port" layoutX="125.0" layoutY="202.0" text=""/>
            <Label fx:id="txt_username" layoutX="125.0" layoutY="235.0" text=""/>
            <Label fx:id="txt_password" layoutX="125.0" layoutY="269.0" text=""/>
            <Label fx:id="txt_database" layoutX="125.0" layoutY="301.0" text=""/>
            <Label fx:id="txt_error" layoutX="145.0" layoutY="333.0" text="">
